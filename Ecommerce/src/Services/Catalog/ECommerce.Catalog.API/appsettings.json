{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DefaultHostName": "(localdb)\\mssqllocaldb",
  "DefaultPassword": "Password",
  "ConnectionStrings": {
    "DefaultConnection": "Server=[HOST];Database=Anadolu_ECommerce_CatalogDb;User ID=sa;Password=[PASS];MultipleActiveResultSets=true; Encrypt=False; Trust Server Certificate=True;"
  },
  "JwtSettings": {
    "SecretKey": "Anadolu-ECommerce-Secret-Key-for-secure",
    "Issuer": "identity-service",
    "Audience": "client",
    "ExpirationInHours": 1
  }

}
